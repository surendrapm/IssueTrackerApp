{
  "name": "@repo/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    ".": "./src/index.ts"
  },
  "scripts": {
    "lint": "eslint src/",
    "build": "tailwindcss -i ./src/styles.css -o dist/index.css",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "generate:component": "turbo gen react-component"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/tailwind-config": "*",
    "@repo/typescript-config": "*",
    "@turbo/gen": "^1.10.12",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@radix-ui/themes": "^2.0.2",
    "zod": "^3.22.4"
  }
}
